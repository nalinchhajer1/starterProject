{
  "name": "solito-blank",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "devDependencies": {
    "@types/react": "^18.2.21",
    "@types/react-native": "^0.73.0",
    "eslint": "^9.0.0",
    "eslint-config-expo": "~9.2.0",
    "expo": "^53.0.0",
    "turbo": "^2.4.4",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "native": "cd apps/expo && yarn start",
    "web": "cd apps/next && yarn next",
    "android": "cd apps/expo && yarn android",
    "ios": "cd apps/expo && yarn ios",
    "lint": "expo lint"
  },
  "packageManager": "yarn@4.9.4",
  "eslintConfig": {
    "extends": "next",
    "settings": {
      "next": {
        "rootDir": "apps/next/"
      }
    },
    "root": true
  },
  "prettier": {
    "semi": false,
    "useTabs": false,
    "tabWidth": 2,
    "singleQuote": true
  }
}
