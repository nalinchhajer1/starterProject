{
  "name": "native-library",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/native-library",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/native-library",
        "main": "packages/native-library/index.ts",
        "tsConfig": "packages/native-library/tsconfig.lib.json",
        "assets": ["packages/native-library/**/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/native-library/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/native-library/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["type:feature", "scope:shared"]
}
