{
    "name": "navigation-ports",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "packages/navigation-ports",
    "projectType": "library",
    "targets": {
        "build": {
            "executor": "@nx/js:tsc",
            "outputs": ["{options.outputPath}"],
            "options": {
                "outputPath": "dist/packages/navigation-ports",
                "main": "packages/navigation-ports/nativeAdapter.ts",
                "tsConfig": "packages/navigation-ports/tsconfig.lib.json",
                "assets": ["packages/navigation-ports/*.md"]
            }
        },
        "lint": {
            "executor": "@nx/eslint:lint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["packages/navigation-ports/**/*.{ts,tsx,js,jsx}"]
            }
        },
        "test": {
            "executor": "@nx/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
            "options": {
                "jestConfig": "packages/navigation-ports/jest.config.js",
                "passWithNoTests": true
            }
        }
    },
    "tags": ["layer:navigation", "scope:shared"]
}
